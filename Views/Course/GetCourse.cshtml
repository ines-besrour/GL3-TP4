@{
    @model IEnumerable<Student>

}


@{
    <h1>@ViewBag.Id</h1>
    <p></p>
    <table class="table">
        <thead>
            <tr>
                <th>Course</th>
            </tr>
        </thead>
        <tbody>
            @{
                @foreach (Student student in Model)
                {
                    <tr>
                        <th>
                            @student.first_name
                        </th>
                    </tr>
                }
            }
        </tbody>
    </table>
       

}
<a asp-controller="Course" asp-action="index" class="alert-info"> GO BACK TO ALL COURSES</a>
