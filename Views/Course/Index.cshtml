@{
    @model IEnumerable<String>

}



    <h1 class="card-header-tabs">All Courses</h1>
    <p></p>
    <table class="table">
        <thead>
            <tr>
                <th>Course</th>
            </tr>
        </thead>
        <tbody>
            @{
                foreach (String course in Model)
            {
                <tr>
                    <th>
                        <a href="Course/getcourse/@course">
                        <p>@course</p>
                        </a>
                    </th>
                </tr>
                }
            }
        </tbody>
    </table>

